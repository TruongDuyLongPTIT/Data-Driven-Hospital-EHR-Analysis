    "table.include.list": "public.admissions,public.caregiver,public.chartevents,public.d_hcpcs,public.d_icd_diagnoses,public.d_icd_procedures,public.d_items,public.d_labitems,public.datetimeevents,public.diagnoses_icd,public.drgcodes,public.emar,public.emar_detail,public.hcpcsevents,public.icustays,public.ingredientevents,public.inputevents,public.labevents,public.microbiologyevents,public.omr,public.outputevents,public.patients,public.pharmacy,public.poe,public.poe_detail,public.prescriptions,public.procedureevents,public.procedures_icd,public.services,public.transfers",

    "plugin.name": "pgoutput",
    "topic.prefix": "postgres",
    "slot.name": "debezium_slot",
    "publication.name": "debezium_publication",
    "publication.autocreate.mode": "disabled",
    "snapshot.mode": "never",
